    "@nuxt/schema": "^3.10.0",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@unocss/core": "^0.58.5",
    "@vueuse/core": "^10.3.0",
    "changelogen": "^0.5.0",
    "consola": "^3.1.0",
    "eslint": "^8.40.0",
    "nuxt": "^3.10.0",
    "nuxt-icon": "^0.6.0",
    "playwright": "^1.34.0",
    "sass": "^1.70.0",
    "unocss": "^0.58.5",
    "vitest": "^1.1.3"
