{
  "name": "nuxt-hydration-checker",
  "version": "0.0.1",
  "description": "Actively warns you about hydration issues",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build && nuxi generate ./client",
    "build:client": "nuxi generate client",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:client": "nuxi dev client",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare client",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools": "^0.3.0",
    "@nuxt/devtools-kit": "^0.4.1",
    "@nuxt/devtools-ui-kit": "^0.2.3",
    "@nuxt/kit": "^3.4.2",
    "@nuxtjs/tailwindcss": "^6.4.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "birpc": "^0.2.4",
    "changelogen": "^0.4.1",
    "consola": "^2.15.3",
    "eslint": "^8.34.0",
    "flatted": "^3.2.7",
    "h3": "^1.6.4",
    "hookable": "^5.4.2",
    "naive-ui": "^2.34.3",
    "nuxt": "3.4.2",
    "sirv": "^2.0.2",
    "tinyws": "^0.1.0",
    "unstorage": "^1.5.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "latest",
    "@nuxt/test-utils": "latest",
    "vitest": "^0.28.5"
  },
  "resolutions": {
    "consola": "^3.0.0"
  }
}
